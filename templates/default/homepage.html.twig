{% extends "base.html.twig" %}

{% block body %}

<h3>hello index page</h3>

<ul>
    {% for story in stories %}
    <a href="{{ path('story', {'slug': story.slug}) }}"><li>{{ story.title }}</li></a>
    {% endfor %}
</ul>

<a href="{{ path('storyForm') }}"><button class="btn btn-primary">add a story</button></a>

{% endblock %}
